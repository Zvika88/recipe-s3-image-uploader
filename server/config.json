{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false,
  "logger": {
    "level": "debug"
  },
  "cloudStoreImages": {
    "contentType": "image/jpeg",
    "smallExt": "_sm.jpg",
    "smallWidth": 75,
    "mediumExt": "_md.jpg",
    "mediumWidth": 500,
    "largeExt": "_lg.jpg",
    "largeWidth": 1200,
    "s3": {}
  }
}
